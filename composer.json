{
    "name": "olcs/olcs-internal",
    "description": "OLCS Internal Web Site",
    "repositories": [
        {
            "packagist": false
        },
        {
            "url": "http://devrepo.shd.ci.nonprod.dvsa.aws",
            "type": "composer"
        },
        {
            "type": "vcs",
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-common.git"
        },
        {
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-transfer.git",
            "type": "vcs"
        },
        {
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-utils.git",
            "type": "vcs"
        },
        {
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-auth.git",
            "type": "vcs"
        },
        {
            "type": "vcs",
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-testhelpers.git"
        },
        {
            "type": "vcs",
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-logging.git"
        },
        {
            "type": "vcs",
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-autoload.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "2.3.9",
        "doctrine/annotations": "v1.2.7",
        "symfony/Filesystem": "v2.6.13",
        "zf-commons/zfc-rbac": "2.4.2",
        "olcs/autoload": "@dev",
        "olcs/OlcsCommon": "dev-release/4.0.11.3",
        "olcs/olcs-transfer": "dev-release/4.0.11",
        "olcs/olcs-utils": "dev-release/4.0.11",
        "olcs/olcs-auth": "dev-release/4.0.11",
        "olcs/olcs-logging": "dev-release/4.0.11",
        "soflomo/purifier": "v0.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0.0",
        "mockery/mockery": "~0.9.3",
        "olcs/olcs-testhelpers": "dev-develop"
    },
    "autoload": {
        "classmap": [
            "module/Admin/src",
            "module/Olcs/src",
            "vendor/olcs/OlcsCommon/Common/src/Common"
        ],
        "psr-4": {
            "Common\\": "vendor/olcs/OlcsCommon/Common/src/Common"
        }
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    }
}
