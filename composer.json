{
    "name": "OLCS Self Serve",
    "description": "OLCS Self Service Web Site",
    "repositories": [{
            "type": "package",
            "package": {
                "name": "olcs/OlcsCommon",
                "version": "dev-develop",
                "source": {
                    "url": "git@gitlab.clb.npm:olcs/olcs-common.git",
                    "type": "git",
                    "reference": "origin/develop"
                }
            }
        }],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "~2.3.1",
        "doctrine/doctrine-orm-module": "0.*",
        "olcs/OlcsCommon": "dev-develop",
        "zendframework/zend-developer-tools": "dev-master"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~1.5.3",
        "phpunit/phpunit": "~4.0.1"
    },
    "autoload": {
        "psr-4": {
            "Common\\": "vendor/olcs/OlcsCommon/Common/src/Common"
        }
    },
    "config": {
      "preferred-install": "dist",
        "github-oauth": {
          "github.com": "5cdf40f8d81a7675fdaa5fc92e9381526ef4db7f"
